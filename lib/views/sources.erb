<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width" />
  <title>le TrafficSpy</title>

  <link rel="stylesheet" href="/css/normalize.css" />
  <link rel="stylesheet" href="/css/foundation.css" />
  <link rel="stylesheet" href="/css/font-awesome.css" />
  <link href='http://fonts.googleapis.com/css?family=Quattrocento:700' rel='stylesheet' type='text/css'>
  

  <script src="js/vendor/custom.modernizr.js"></script>

</head>
<body>
  <nav class="top-bar">
    <ul class="title-area">
      <!-- Title Area -->
      <li class="name">
        <h1><a href="#">le TrafficSpy</a></h1>
      </li>
      <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
      <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
    </ul>

    <section class="top-bar-section">
      <!-- Right Nav Section -->
      <ul class="right">
        <li class="divider show-for-small"></li>
        <li class="has-form">
          <a class="button" href="#"><i class="icon-user"></i>Log In</a>
        </li>
      </ul>
    </section>
  </nav>

  <div id="main" class="row">
    <div id="sub-nav" class="large-2 columns">
      <ul class="sub-nav">
        <li><a href="#" class="active"><i class="icon-dashboard"></i>Dashboard</a></li>
        <li><a href="#"><i class="icon-copy"></i>Pages</a></li>
        <li><a href="#"><i class="icon-time"></i>Response Times</a></li>
        <li><a href="#"><i class="icon-money"></i>Campaigns</a></li>
        <li><a href="#"><i class="icon-group"></i>Events</a></li>
        <li><a href="#"><i class="icon-cogs"></i>Operating Systems</a></li>
        <li><a href="#"><i class="icon-laptop"></i>Screen Resolutions</a></li>
      </ul>
    </div>

    <div id="content" class="large-10 columns">
      <div id="content-inner">
        <div class="widget">
          <h3>Pages</h3>
          <table cellspacing='0'> <!-- cellspacing='0' is important, must stay -->
            <!-- Table Header -->
            <thead>
              <tr>
                <th>URL</th>
                <th>Requests</th>
              </tr>
            </thead>
            <!-- Table Header -->

            <!-- Table Body -->
            <tbody>
              <% @sorted_urls.each do |url, count| %>
              <tr>
                <td><a href="#"><%= url %></a></td>
                <td><%= count %></td>
              </tr><!-- Table Row -->
              <% end %>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>100</td>
              </tr><!-- Darker Table Row -->

              <tr>
                <td><a href="#">http://jumpstartlab.com/frankthetank</a></td>
                <td>90</td>
              </tr>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/railzzz</a></td>
                <td>80</td>
              </tr>

              <tr>
                <td><a href="#">http://jumpstartlab.com/tutorialzz</a></td>
                <td>80</td>
              </tr>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/washingtondc4lyfe</a></td>
                <td>60</td>
              </tr>

              <tr>
                <td><a href="#">http://jumpstartlab.com/boom</a></td>
                <td>40</td>
              </tr>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/roobeee</a></td>
                <td>20</td>
              </tr>

            </tbody>
            <!-- Table Body -->

          </table>
        </div>
        <div class="widget">
          <h3>Response Times</h3>
          <table cellspacing='0'> <!-- cellspacing='0' is important, must stay -->
            <!-- Table Header -->
            <thead>
              <tr>
                <th>URL</th>
                <th>Average</th>
              </tr>
            </thead>
            <!-- Table Header -->

            <!-- Table Body -->
            <tbody>

              <tr>
                <td><a href="#">http://jumpstartlab.com/ohyeah</a></td>
                <td>500ms</td>
              </tr><!-- Table Row -->

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>450ms</td>
              </tr><!-- Darker Table Row -->

              <tr>
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>300ms</td>
              </tr>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>200ms</td>
              </tr>

              <tr>
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>200ms</td>
              </tr>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>100ms</td>
              </tr>

              <tr>
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>75ms</td>
              </tr>

              <tr class="even">
                <td><a href="#">http://jumpstartlab.com/whoa</a></td>
                <td>70ms</td>
              </tr>

            </tbody>
            <!-- Table Body -->

          </table>
        </div>
      </div>
    </div>
  </div>

  <script>
  document.write('<script src=' +
  ('__proto__' in {} ? 'js/vendor/zepto' : 'js/vendor/jquery') +
  '.js><\/script>')
  </script>
  
